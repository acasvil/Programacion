package bucle2;

public class Bucle2 {

	public static void main(String[] args) {
		
		int numerogran = 320;
		int numeropeq = 160;
		int restador = 20;
		int acumulador = 0;
		
		while (restador > 160)
		{
			System.out.println(acumulador);
			acumulador = numerogran - restador;
			System.out.println(acumulador);
			
		}

	}

}
